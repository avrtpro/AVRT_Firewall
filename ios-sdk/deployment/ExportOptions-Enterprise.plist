<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<!--
    AVRT SDK - Enterprise Distribution Export Options
    For TestFlight Private Enterprise Evaluation

    (c) 2025 Jason I. Proper, BGBH Threads LLC
-->
<dict>
    <!-- Distribution Method: app-store-connect for TestFlight -->
    <key>method</key>
    <string>app-store-connect</string>

    <!-- Team Configuration -->
    <!-- IMPORTANT: Replace with your Apple Developer Team ID -->
    <key>teamID</key>
    <string>YOUR_TEAM_ID</string>

    <!-- Destination: upload directly to App Store Connect -->
    <key>destination</key>
    <string>upload</string>

    <!-- Signing -->
    <key>signingStyle</key>
    <string>automatic</string>

    <!-- Symbols -->
    <key>uploadSymbols</key>
    <true/>
    <key>stripSwiftSymbols</key>
    <true/>

    <!-- Bitcode (disabled for modern iOS) -->
    <key>compileBitcode</key>
    <false/>
    <key>uploadBitcode</key>
    <false/>

    <!-- Thinning -->
    <key>thinning</key>
    <string>&lt;none&gt;</string>

    <!-- Provisioning Profiles (automatic) -->
    <key>provisioningProfiles</key>
    <dict>
        <key>com.bgbhthreads.avrt.ios</key>
        <string>AVRT iOS Distribution</string>
        <key>com.bgbhthreads.avrt.sdk</key>
        <string>AVRT SDK Framework</string>
    </dict>

    <!-- TestFlight Internal Distribution -->
    <key>manageAppVersionAndBuildNumber</key>
    <true/>
    <key>generateAppStoreInformation</key>
    <false/>

    <!-- Installer Signing -->
    <key>installerSigningCertificate</key>
    <string>Apple Distribution</string>
</dict>
</plist>
